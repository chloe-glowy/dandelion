{
  "name": "community-aid-app",
  "version": "0.6.0",
  "scripts": {
    "start": "node server-build/server/root/runserver.js",
    "dev:client": "yarn install && npm run web",
    "dev:server": "yarn install && MONGODB_DB_NAME=\"AidApp-Test\" MONGO_AID_REQUEST_INDEX_NAME=\"aid-request-test\" HOT_RELOAD=True DEBUG=server nodemon --config nodemon.json src/server/root/runserver.ts",
    "dev:server:prod": "yarn install && MONGODB_DB_NAME=\"AidApp\" MONGO_AID_REQUEST_INDEX_NAME=\"aid-requests\" HOT_RELOAD=True DEBUG=server nodemon --config nodemon.json src/server/root/runserver.ts",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watchAll",
    "prettier": "prettier",
    "eslint": "eslint",
    "build:client": "expo export:web",
    "build:graphql": "apollo client:codegen --endpoint=http://localhost:3333/graphql --target=typescript",
    "build:server": "./build_server.sh",
    "deploy:local:all": "npm run deploy:local:ios:all; npm run deploy:local:android:all",
    "deploy:local:ios:all": "npm run deploy:local:ios:preview; npm run deploy:local:ios:development; npm run deploy:local:ios:production",
    "deploy:local:ios:preview": "eas build --platform ios --profile preview",
    "deploy:local:ios:development": "eas build --platform ios --profile development",
    "deploy:local:ios:production": "eas build --platform ios --profile production",
    "deploy:local:android:all": "npm run deploy:local:android:preview; npm run deploy:local:android:development; npm run deploy:local:android:production",
    "deploy:local:android:preview": "eas build --platform android --profile preview",
    "deploy:local:android:development": "eas build --platform android --profile development",
    "deploy:local:android:production": "eas build --platform android --profile production",
    "scripts:set-precomputed-fields:test": "npm run build:server && MONGODB_DB_NAME=\"AidApp-Test\" node server-build/server/scripts/set-precomputed-fields.js",
    "scripts:set-precomputed-fields:prod": "npm run build:server && MONGODB_DB_NAME=\"AidApp\" node server-build/server/scripts/set-precomputed-fields.js",
    "scripts:backfill-who-is-working-on-it": "npm run build:server && MONGODB_DB_NAME=\"AidApp\" node server-build/server/scripts/backfill-who-is-working-on-it.js",
    "scripts:unnest-details:test": "npm run build:server && MONGODB_DB_NAME=\"AidApp-Test\" node server-build/server/scripts/unnest-details.js",
    "scripts:unnest-details:prod": "npm run build:server && MONGODB_DB_NAME=\"AidApp\" node server-build/server/scripts/unnest-details.js",
    "scripts:migrate-sharing-group-from-crew:test:dry": "npm run build:server && IS_DRY_RUN=\"True\" MONGO_AID_REQUEST_INDEX_NAME=\"aid-request-test\" MONGODB_DB_NAME=\"AidApp-Test\" node server-build/server/scripts/migrate-sharing-group-from-crew/migrate-sharing-group-from-crew.js",
    "scripts:migrate-sharing-group-from-crew:test:live": "npm run build:server && IS_DRY_RUN=\"False\" MONGO_AID_REQUEST_INDEX_NAME=\"aid-request-test\" MONGODB_DB_NAME=\"AidApp-Test\" node server-build/server/scripts/migrate-sharing-group-from-crew/migrate-sharing-group-from-crew.js",
    "scripts:migrate-sharing-group-from-crew:prod:dry": "npm run build:server && IS_DRY_RUN=\"True\" MONGO_AID_REQUEST_INDEX_NAME=\"aid-requests\" MONGODB_DB_NAME=\"AidApp\" node server-build/server/scripts/migrate-sharing-group-from-crew/migrate-sharing-group-from-crew.js",
    "scripts:migrate-sharing-group-from-crew:prod:live": "npm run build:server && IS_DRY_RUN=\"False\" MONGO_AID_REQUEST_INDEX_NAME=\"aid-requests\" MONGODB_DB_NAME=\"AidApp\" node server-build/server/scripts/migrate-sharing-group-from-crew/migrate-sharing-group-from-crew.js"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "verbose": true,
    "rootDir": "",
    "modulePaths": [
      "<rootDir>",
      "<rootDir>/node_modules"
    ]
  },
